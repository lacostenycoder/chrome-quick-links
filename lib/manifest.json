{
  "manifest_version": 2,
  "name": "Quick Text Linker",
  "description": "Type Keyboard Shortcut To a Website",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Enter Shortcut"
  },
  "incognito": "spanning",
  "permissions": [
    "tabs",
    "storage"
  ],
  "web_accessible_resources": [
        "style.css",
        "list.js",
        "vendor/bootstrap.min.css"
  ]
}
